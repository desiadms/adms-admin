		"@total-typescript/ts-reset": "^0.5.1",
		"@types/leaflet": "^1.9.14",
		"@types/react": "^18.3.12",
		"@types/react-dom": "^18.3.1",
		"@vitejs/plugin-react-swc": "^3.7.1",
		"autoprefixer": "^10.4.20",
		"codegen": "^0.1.0",
		"graphql-codegen-typescript-validation-schema": "^0.17.0",
		"npm-run-all": "^4.1.5",
		"postcss": "^8.4.47",
